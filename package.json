{
  "name": "slideshow-react",
  "version": "0.0.2",
  "main": "main.js",
  "scripts": {
    "electron-dev": "concurrently --kill-others \"npm run electron-server\" \"npm run start-electron\"",
    "electron-server": "cross-env HOT=1 NODE_ENV=development TARGET=electron node -r babel-register server.js",
    "hot-server": "cross-env HOT=1 NODE_ENV=development TARGET=local node -r babel-register server.js",
    "start": "cross-env NODE_ENV=production TARGET=electron electron ./",
    "start-electron": "cross-env HOT=1 NODE_ENV=development TARGET=electron electron -r babel-register -r babel-polyfill ./",
    "stage": "cross-env NODE_ENV=stage webpack -d --watch --config webpack.config.stage.babel.js --progress --profile --colors",
    "build": "cross-env NODE_ENV=production webpack -p --config webpack.config.production.babel.js",
    "dev": "cross-env HOT=1 NODE_ENV=development node -r babel-register server.js",
    "screeners-dev": "cross-env HOT=1 TARGET=screeners NODE_ENV=development node -r babel-register server.js"
  },
  "browser": {
    "vertx": false
  },
  "bin": {
    "electron": "./node_modules/.bin/electron"
  },
  "devDependencies": {
    "babel-core": "^6.14.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-dev-expression": "^0.2.1",
    "babel-plugin-webpack-loaders": "^0.9.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.14.0",
    "concurrently": "^3.0.0",
    "cross-env": "^3.0.0",
    "css-loader": "^0.26.2",
    "ejs": "^2.5.2",
    "electron": "^1.3.4",
    "electron-devtools-installer": "^2.0.1",
    "electron-packager": "^8.1.0",
    "electron-rebuild": "^1.2.1",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^2.0.0",
    "file-loader": "^0.10.1",
    "json-loader": "^0.5.4",
    "node-sass": "^4.5.2",
    "redux-devtools": "^3.2.0",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-log-monitor": "^1.0.9",
    "redux-logger": "^2.6.1",
    "sass-loader": "^6.0.2",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^2.2.1",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.12.2",
    "webpack-livereload-plugin": "^0.10.0",
    "webpack-merge": "^3.0.0",
    "webpack-validator": "^2.2.7"
  },
  "dependencies": {
    "axios": "^0.15.3",
    "babel-polyfill": "^6.13.0",
    "create-react-class": "^15.5.3",
    "electron-debug": "^1.0.1",
    "dom-helpers": "^3.2.1",
    "element-resize-detector": "^1.1.12",
    "fingerprintjs": "^0.5.3",
    "hls.js": "^0.7.2",
    "iframe-resizer": "^3.5.11",
    "interact.js": "^1.2.6",
    "invariant": "^2.2.1",
    "jbinary": "^2.1.3",
    "js-cookie": "^2.1.0",
    "keycode": "^2.1.4",
    "lodash.debounce": "^4.0.8",
    "lodash.difference": "^4.5.0",
    "lodash.foreach": "^4.5.0",
    "lodash.throttle": "^4.0.1",
    "moment": "^2.17.1",
    "prop-types": "^15.5.10",
    "react": "^15.3.2",
    "react-addons-css-transition-group": "^15.6.0",
    "react-addons-perf": "^15.4.2",
    "react-dom": "^15.3.2",
    "react-overlays": "^0.7.0",
    "react-prop-types": "^0.4.0",
    "react-redux": "^5.0.3",
    "react-router": "^3.0.5",
    "react-router-redux": "^4.0.6",
    "react-transition-group": "^1.1.3",
    "redux": "^3.5.2",
    "redux-promise": "^0.5.3",
    "redux-thunk": "^2.0.1",
    "uncontrollable": "^4.0.1",
    "url-toolkit": "^1.0.4",
    "warning": "^3.0.0",
    "webworkify": "^1.4.0",
    "webworkify-webpack": "^2.0.1"
  }
}
